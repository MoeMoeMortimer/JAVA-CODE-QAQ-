package com.ouc.mortimer.sep23;

import java.util.Scanner;

public class CF719B {
	public static int lmt;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		while (sc.hasNext()) {
			int n = sc.nextInt();
			int t = sc.nextInt();
			String score = sc.next();
			String res;
			int flag = score.indexOf('.');
			char[] arr = score.toCharArray();
			char[] front = score.substring(0, flag).toCharArray();
			lmt = n;
			for (int j = flag + 1; j < n; j++) {
				if (arr[j] >= '5') {
					lmt = j;
					break;
				}
			}
			if(lmt != n){
				for (int i = 0, j = lmt; i < t && j > flag; i++, j--) {
					if (arr[j] >= '5') {
						if (j - 1 != flag) {
							arr[j - 1]++;
							lmt = j;
						} else {
							lmt = flag;
							break;
						}
					}
				}
			}
			if (lmt == flag) {
				check(front, lmt);
				res = new String(front);
				if (lmt == 0) 
					res = "1" + res;
			} else {
				res = new String(arr);
				res = res.substring(0, lmt);
			}
			System.out.println(res);
		}
		sc.close();
	}

	private static void check(char[] s, int len) {
	    if(len > 0){
	    	if(s[len-1] == '9'){
		    	s[len-1] = '0';
		    	check(s, len-1);
		    } else {
		    	s[len-1]++;
		    	return ;
		    }
	    }
	}
}
